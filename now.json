{
  "version": 2,
  "name": "covaid",
  "alias": "covaid.now.sh",
  "builds": [
    {
      "src": "client/package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/client/$1",
      "continue": true
    }
  ]
}
